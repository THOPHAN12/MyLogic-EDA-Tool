    "dce": {
      "enabled": true,
      "test_file": "tests/algorithms/test_dce.py",
      "description": "Dead Code Elimination algorithm tests"
    },
    "cse": {
      "enabled": true,
      "test_file": "tests/algorithms/test_cse.py",
