{
  "test_suite": {
    "name": "MyLogic EDA Tool Test Suite",
    "version": "1.0.0",
    "description": "Comprehensive test suite for MyLogic EDA Tool algorithms"
  },
  "algorithms": {
    "strash": {
      "enabled": true,
      "test_file": "tests/algorithms/test_strash.py",
      "description": "Structural Hashing algorithm tests"
